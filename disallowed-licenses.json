{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "packageRules": [
        {
            "matchManagers": [ 
              "nuget" 
            ],
            "matchPackageNames": [ "EPPlus" ],
            "allowedVersions": "/^[0-4]\\.[0-9]+\\.[0-9]+(\\.[0-9]+)?$/"
        },
        {
            "matchManagers": [ 
              "nuget" 
            ],
            "matchPackageNames": [ "FluentAssertions" ],
            "allowedVersions": "/^[0-7]\\./"
        }
    ]
  }
  