{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/.sqlproj$/"
      ],
      "matchStrings": [
        "<Sdk\\s+Name=\"(?<depName>.*?)\"\\s+Version=\"(?<currentValue>.*?)\"\\s*/>"
      ],
      "datasourceTemplate": "nuget",
      "versioningTemplate": "nuget"
    }
  ]
}